{
    "manifest_version": 3,
    "name": "ZP Insider",
    "version": "0.0.1",
    "description": "Discover salaries using ZP Insider extension",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "128": "icons/icon128.png"
    },
    "host_permissions": [
        "http://localhost:3000/*"
    ]
}
